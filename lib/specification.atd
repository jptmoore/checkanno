
type specification = {
  t <json name="type"> : [ Annotation ];
  motivation : [ Motivation <json name="commenting"> ]; 
  body : body_t;
  target : string
}

type body_t = {
  t <json name="type"> : [ TextualBody ];
  language : [ Language  <json name="en"> ];
  format : [ Format <json name="text/plain"> ];
  value : string
}
